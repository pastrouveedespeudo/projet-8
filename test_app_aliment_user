"""Test functions"""

import mes_aliments.mes_aliments_preferer_user  as script

def test_mes_aliment_user():
    """test function food user"""

    parameter = 'tetetete'
    out = ['chocolat au lait', 'crêpes dentelle au chocolat au lait', 'dégustation riz soufflé', 'chocolat noir 70%', '15 mini quiches', 'assortiment de chocolats']
    assert script.mes_aliment_user(parameter) == out


